{
  "name": "vue-flext",
  "version": "1.1.5",
  "description": "Handlebars on Vue",
  "keywords": [
    "templates",
    "templating engine",
    "handlebars on vue"
  ],
  "type": "module",
  "main": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/index.css"
  },
  "scripts": {
    "lint": "eslint",
    "build:types": "vue-tsc --noEmit false",
    "build:css": "tailwindcss -i ./src/index.tail.css -o ./dist/index.css --minify",
    "build": "vite build && npm run build:types && npm run build:css",
    "prepare": "npm run build"
  },
  "files": [
    "dist/"
  ],
  "author": "Kenny Romanov",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.30.1",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "esbuild": "^0.25.1",
    "esbuild-plugin-import-glob": "^0.1.1",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.0"
  },
  "dependencies": {
    "@trustme24/flext": "^1.1.3",
    "vue": "^3.5.17",
    "vue-a4": "^1.0.2"
  }
}
