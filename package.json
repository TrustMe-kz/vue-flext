{
  "name": "vue-flext",
  "version": "1.4.0",
  "description": "A Powerful Templating Engine",
  "keywords": ["templates", "templating engine", "modular templates", "handlebars"],
  "type": "module",
  "main": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/vue-flext.css"
  },
  "scripts": {
    "lint": "eslint",
    "start:test-app": "cd test/app && npm run test-app",
    "build:types": "vue-tsc --noEmit false",
    "build": "vite build && npm run build:types",
    "test:app": "npm run build && cd test/app && npm i && npm run test-app",
    "prepare": "npm run build"
  },
  "files": [
    "dist/"
  ],
  "author": "Kenny Romanov",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "eslint-plugin-vue": "^10.6.1",
    "globals": "^16.5.0",
    "sass-embedded": "^1.93.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.5"
  },
  "peerDependencies": {
    "@trustme24/flext": "^1.7.0"
  },
  "dependencies": {
    "vue": "^3.5.25",
    "vue-a4": "^1.0.2"
  }
}
